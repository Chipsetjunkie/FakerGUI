{
  "name": "@chipsetjunkie/fakergui",
  "private": false,
  "version": "1.3.4",
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "repository": "https://github.com/Chipsetjunkie/vite-library-template",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist && tsc --p ./tsconfig-build.json && vite build",
